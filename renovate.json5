{
  "extends": [
    "github>getstrm/renovate-config//base-renovate-config.json5"
  ],
  packageRules: [
    {
      matchManagers: [
        "gomod"
      ],
      matchPackagePrefixes: [
        "build.buf.gen"
      ],
      versioning: "regex:^v(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)-(?<compatibility>.*)\\.(?<revision>\\d+)$"
    }
  ]
}
